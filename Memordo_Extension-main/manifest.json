{
  "manifest_version": 3,
  "name": "memordo Extension",
  "version": "1.0",
  "description": "This is a memordo extension.",
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icons/app_icon_16.png",
      "32": "icons/app_icon_32.png",
      "48": "icons/app_icon_48.png",
      "128": "icons/app_icon_128.png"
    },
    "default_title": "memordo"
  },
  "icons": {
    "16": "icons/app_icon_16.png",
    "32": "icons/app_icon_32.png",
    "48": "icons/app_icon_48.png",
    "128": "icons/app_icon_128.png",
    "512": "icons/app_icon_512.png"
  },
  "background": {
    "service_worker": "background/background.js"
  },
  "permissions": [
    "tabs",
    "storage",
    "identity"
  ],
  "oauth2": {
    "client_id": "608441152139-vsd5c67k4ep8s208fja9s7q3dsum84ls.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file"
    ]
  },
  "host_permissions": [
    "<all_urls>",
    "https://www.googleapis.com/"
  ],
  "web_accessible_resources": [
    {
      "resources": ["history/history.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
